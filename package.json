{
    "name": "ai-assistant",
    "version": "1.0.0",
    "description": "AI Assistant project with Google JavaScript Style Guide",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.mjs",
        "dev": "nodemon src/index.mjs",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "test": "node --test",
        "test:dev": "node --test \"tests/dev/*.test.*\"",
        "test:qa": "node --test \"tests/qa/*.test.*\"",
        "prepare": "husky install"
    },
    "keywords": [
        "ai",
        "assistant",
        "javascript",
        "node"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^8.57.0",
        "eslint-config-google": "^0.14.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "nodemon": "^3.0.2"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.23.0",
        "express": "^5.1.0",
        "zod": "^4.1.13"
    },
    "lint-staged": {
        "src/**/*.js": [
            "eslint --fix"
        ]
    }
}
